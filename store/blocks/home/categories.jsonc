{
    "flex-layout.row#categories": {
        "title": "Categorias destacadas",
        "children": [
            "slider-layout#categories"
        ],
        "props": {
            "blockClass": "categories"
        }
    },
    "slider-layout#categories": {
        "children": [
            "flex-layout.col#smart-tv",
            "flex-layout.col#celulares",
            "flex-layout.col#climatizacion",
            "flex-layout.col#lavarropas",
            "flex-layout.col#heladeras",
            "flex-layout.col#cocinas",
            "flex-layout.col#termotanques"
        ],
        "props": {
            "itemsPerPage": {
                "desktop": 7,
                "tablet": 4,
                "phone": 1
            },
            "showPaginationDots": "never",
            "infinite": true,
            "fullWidth": false,
            "blockClass": "categories"
        }
    },
